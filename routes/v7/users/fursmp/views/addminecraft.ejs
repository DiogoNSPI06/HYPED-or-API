    <script>
      async function submitForm() {
        const formElement = document.querySelector('#formulario form');
        const queryParam = new URLSearchParams(new FormData(formElement)).toString();

        if (queryParam === "minecraft_nickname=")
            return;

        await fetch(`https://api.hypeds.com/v7/users/addnickname/<%= userid %>?${queryParam}`)
        window.location.href = "https://api.hypeds.com/v7/users/fursmp/profiles/@me"
      }
    </script>
